# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Product {
  id: ID!
  name: String!
  price: Int!
  imageUrl: String!
  createdAt: String!
  updatedAt: String!
}

type Query {
  products: [Product]!
}

input NewProduct {
  name: String!
  price: Int!
  imageUrl: String!
}

type Mutation {
  createProduct(input: NewProduct!): Product!
}
